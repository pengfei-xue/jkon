{
  "name": "Want JK as page down/up",
  "manifest_version": 2,
  "version": "0.1",
  "description": "Creates a keyboard shortcut (J & K) to page down/up",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
                "jquery/jquery-1.7.min.js", 
                "action.js"
            ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions" : ["<all_urls>"]
}
