{
  "name": "Want JK as page down/up",
  "version": "0.1",
  "description": "Creates a keyboard shortcut (J & K) to page down/up",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
                 "jquery/jquery-1.6.3.min.js", 
                 "jquery/jquery-ui-1.8.16.custom.min.js",
                 "action.js"
            ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions" : ["<all_urls>"]
}
